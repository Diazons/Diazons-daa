      "source": [
        "#buatlah suatu fungsi untuk mengurangi 2 himpunan angka, jika getSum untuk menjumlahkan angka\n",
        "def getKurang(myList):\n",
        "  sum = 70\n",
        "  for row in myList:\n",
        "    for item in row:\n",
        "      sum -= item\n",
        "  return sum"
      ]
    },
